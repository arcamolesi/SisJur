@model IEnumerable<SisJur.Models.Vara>

@{
    ViewData["Title"] = "Varas";
    Layout = "~/Views/Shared/_LayoutManter.cshtml";
}

<h1>Varas</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Processo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Processo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Advogado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.juiz)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.valorcausa)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Processo.id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Processo.descricao)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Advogado.nome)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.juiz)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valorcausa)
                </td>

            </tr>
        }
    </tbody>
</table>
